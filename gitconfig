[diff]
	tool = araxis
[difftool "diffmerge"]
	cmd = diffmerge \"$LOCAL\" \"$REMOTE\"
[merge]
 	tool = araxis
[mergetool "diffmerge"]
	cmd = diffmerge --merge --result=\"$MERGED\"\n  \"$LOCAL\" \"$BASE\" \"$REMOTE\"
	trustexitcode = false
