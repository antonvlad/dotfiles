{
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 13,
    "editor.fontFamily": "Source Code Pro, SF Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.scrollBeyondLastColumn": 0,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.gitExplorer.files.layout": "list",
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.associations": {
        "*.h": "objective-c",
        "*.m": "objective-c",
        "*.dvticonspecs": "json"
    },
    "window.zoomLevel": 0,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.alwaysSignOff": true,
    "git.defaultCloneDirectory": "/source/",
    "[git-commit]": {
        "editor.rulers": [
            100
        ]
    },
    "editor.rulers": [
        100
    ],
    "sourcekit-lsp.serverPath": "/source/sourcekit-lsp/.build/debug/sourcekit-lsp",
    "sourcekit-lsp.toolchainPath": "/Volumes/Xcode/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/",
    "[objective-c]": {
        
    },
    "gitlens.views.repositories.files.layout": "list",
    "gitlens.mode.active": "zen",
    "editor.multiCursorModifier": "ctrlCmd"
}